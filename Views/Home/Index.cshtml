@model DataGov_API_Intro.Models.Parks

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>

    
</div>
<div class="text-left">
    <h2 class="display-4">Parks</h2>
    <ul>

        @foreach (DataGov_API_Intro.Models.Park park in Model.data)
        {
            <li>@("[" + park.parkCode + "] - " +  park.fullName + " - {" + park.latLong + "}")</li>
        }
    </ul>
</div>